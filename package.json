{
  "name": "vite-vue-ssr",
  "version": "0.0.1",
  "packageManager": "pnpm@7.4.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "node -r @swc-node/register server",
    "build": "npm run build:client && npm run build:server",
    "preview": "npx http-server dist/static",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssg": "vite build --ssrManifest --outDir dist/static && npm run build:server && node -r @swc-node/register prerender",
    "server": "cross-env NODE_ENV=production node -r @swc-node/register server",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@vueuse/head": "^0.7.7",
    "axios": "^0.24.0",
    "pinia": "^2.0.17",
    "vant": "^3.5.2",
    "vue": "3",
    "vue-router": "4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@swc-node/register": "^1.5.1",
    "@types/koa": "^2.13.5",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^18.6.2",
    "@vitejs/plugin-vue": "^1.10.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "http-server": "^14.0.0",
    "koa": "^2.13.4",
    "koa-connect": "^2.1.0",
    "koa-static": "^5.0.0",
    "pnpm": "7.4.0",
    "typescript": "^4.7.4",
    "vite": "^2.7.2"
  }
}
